<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.UserVersionMapper">

    <select id="queryById" resultType="com.example.demo.domain.UserVersion">
        SELECT *
        FROM user_version
        WHERE id = #{id}
    </select>

</mapper>